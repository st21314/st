package com.itheima.student.study20;

public class Ddddd {
	 public static void main(String[] args) {
		int[]  a={9};
		int[] b = a;
		a[0] = 7;

		System.out.println(a==b);
		for (int i : b) {
			System.out.println(i);
		}
		
		
		Integer i = 8;
		Integer j = i;
		i=5;
		System.out.println(j);
	}
}
