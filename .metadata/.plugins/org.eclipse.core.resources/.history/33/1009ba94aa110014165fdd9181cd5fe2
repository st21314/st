package com.itheima.student.study20;

public class Testtttttttttt {
	public static void main(String[] args) {
		for (int i = 0; i < 15; i++) {
			for (int j = i; j < 15; j++) {
				System.out.print(" ");

			}

			for (int j = 0; j < i; j++) {
				System.out.print("* ");

			}
			System.out.println();
		}
	}
}
