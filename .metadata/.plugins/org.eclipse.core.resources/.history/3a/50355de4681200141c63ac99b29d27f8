package com.itheima.student.study20;

public class Aaaaa {
	public static void main(String[] args) {
		out();
	}

	private static void out() {
		System.out.println("0000\n0111\n0123\n0123");
		System.out.println("****");
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 4; j++) {
				System.out.print(i < j ? i : j);
			}
			System.out.println();
		}
	}
}
